PORT=3000

# Environment mode: development | production
NODE_ENV=development

# CORS allowed origins (comma-separated)
CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Frontend URL for redirects
FRONTEND_URL=http://localhost:3000

# Appwrite Cloud endpoint (get from Appwrite Console)
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1

# Appwrite Project ID (get from Appwrite Console)
APPWRITE_PROJECT_ID=your_project_id_here

# Appwrite API Key with appropriate permissions
APPWRITE_API_KEY=your_api_key_here

# Appwrite Database ID
APPWRITE_DATABASE_ID=axon_production


# Development: Local Redis host
REDIS_HOST=localhost

# Development: Local Redis port
REDIS_PORT=6379

# Production: AWS Redis host (leave empty for development)
# REDIS_HOST=your-aws-redis-host.cache.amazonaws.com

# Production: AWS Redis port
# REDIS_PORT=6379

# Production: AWS Redis password
# REDIS_PASSWORD=your_redis_password_here


# Arweave wallet file path (production)
# ARWEAVE_WALLET_PATH=./wallet.json

# Arweave gateway URL
# ARWEAVE_GATEWAY=https://arweave.net

# Service wallet address for verification
# SERVICE_WALLET_ADDRESS=your_wallet_address_here

# ArLocal port for development (default: 1984)
# ARLOCAL_PORT=1984


# HNSW algorithm parameters for vector search
# EIZEN_M=16
# EIZEN_EF_CONSTRUCTION=200
# EIZEN_EF_SEARCH=50